### View

### View

view:aView 
    rdf:type archimate:View ;
    skos:prefLabel "An example of a view" ;
    skos:definition "An ArchiMate view containing a depiction of all ArchiMate elements." ;
    archimate:contains
        # STRATEGY
        view:aCapability ,
        view:aCourseOfAction ,
        view:aResource ,
        view:aValueStream ,
        # BUSINESS
        view:aBusinessActor ,
        view:aBusinessRole ,
        view:aBusinessCollaboration ,
        view:aBusinessInterface ,
        view:aBusinessProcess ,
        view:aBusinessFunction ,
        view:aBusinessInteraction ,
        view:aBusinessEvent ,
        view:aBusinessService ,
        view:aBusinessObject ,
        view:aContract ,
        view:aRepresentation ,
        view:aProduct ,
        # APPLICATION
        view:anApplicationComponent ,
        view:anApplicationCollaboration ,
        view:anApplicationInterface ,
        view:anApplicationFunction ,
        view:anApplicationInteraction ,
        view:anApplicationProcess ,
        view:anApplicationService ,
        view:anApplicationEvent ,
        view:aDataObject ,
        # TECHNOLOGY & PHYSICAL
        view:aNode ,
        view:aDevice ,
        view:aSystemSoftware ,
        view:aTechnologyCollaboration ,
        view:aTechnologyInterface ,
        view:aTechnologyFunction ,
        view:aTechnologyProcess ,
        view:aTechnologyInteraction ,
        view:aTechnologyService ,
        view:aTechnologyEvent ,
        view:anArtifact ,
        view:aFacility ,
        view:aDistributionNetwork ,
        view:aMaterial ,
        view:aPath ,
        view:aCommunicationNetwork ,
        # MOTIVATION
        view:anAssessment ,
        view:aDriver ,
        view:aGoal ,
        view:anOutcome ,
        view:aPrinciple ,
        view:aRequirement ,
        view:aConstraint ,
        view:aMeaning ,
        view:aValue ,
        view:aStakeholder ,
        # IMPLEMENTATION & MIGRATION
        view:aDeliverable ,
        view:aWorkPackage ,
        view:aPlateau ,
        view:aGap ,
        view:anImplementationEvent ,
        # GENERIC
        view:aGrouping ,
        view:aLocation
    ;
    rdfs:isDefinedBy model:allElements .

