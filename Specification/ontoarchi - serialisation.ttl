### Logic

shp:Element
    a sh:NodeShape;
    sh:rule rule:Element;
    sh:targetClass ontoarchi:Element;
    skos:prefLabel 'element shape'@en;
    rdfs:isDefinedBy ontoarchi:.

  
  rule:Element
    a sh:SPARQLRule;
    rdfs:comment 'A rule that creates an archimate fragment based on an ontoarchi element.'@en;
    skos:prefLabel 'ontoarchi element rule'@en;
    sh:construct '''

prefix archimate: <https://data.rijksfinancien.nl/archimate/model/def/>
prefix function: <https://data.rijksfinancien.nl/ontoarchi/model/function/>
prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>
prefix ontoarchi: <https://data.rijksfinancien.nl/ontoarchi/model/def/>
prefix xml: <http://www.w3.org/XML/1998/namespace#>
prefix xsd: <http://www.w3.org/2001/XMLSchema#>
prefix xsi: <http://www.w3.org/2001/XMLSchema-instance#>

construct {

  # create the ontoarchi-fragment for the ontoarchi-element to be serialized.
  ?element rdf:type archimate:Element;
           archimate:identifier ?identifier;
           xsi:type ?xsiType;
           rdf:_1 ?elementNameNode.
   ?elementNameNode rdf:type archimate:Name;
           archimate:name ?archimateName;
           xml:lang "en".

} where {

  # Get the ontoarchi tag of the ontoarchi-element to be serialized.
  $this skos:prefLabel ?archimateName.
  $this rdf:type ?ontoarchiElement.
  ?ontoarchiElement rdfs:subClassOf ontoarchi:Element;
    xsi:type ?xsiType.
  

''';
    rdfs:isDefinedBy ontoarchi:.

shp:Relationship
    a sh:NodeShape;
    sh:rule rule:Relationship;
    sh:targetSubjectsOf ontoarchi:relationship;
    skos:prefLabel 'relationship shape'@en;
    rdfs:isDefinedBy ontoarchi:.

rule:Relationship
    a sh:SPARQLRule;
    rdfs:comment 'A rule that creates an archimate fragment based on an ontoarchi relationship.'@en;
    skos:prefLabel 'ontoarchi relationship rule'@en;
    sh:construct '''

prefix archimate: <https://data.rijksfinancien.nl/archimate/model/def/>
prefix function: <https://data.rijksfinancien.nl/ontoarchi/model/function/>
prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>
prefix ontoarchi: <https://data.rijksfinancien.nl/ontoarchi/model/def/>
prefix xml: <http://www.w3.org/XML/1998/namespace#>
prefix xsd: <http://www.w3.org/2001/XMLSchema#>
prefix xsi: <http://www.w3.org/2001/XMLSchema-instance#>

construct {

  # create the ontoarchi-fragment for the ontoarchi-relationship
  ?relationship rdf:type archimate:Relationship;
           archimate:identifier ?identifier;
           xsi:type ?xsiType;
           rdf:_1 ?relationshipNameNode.
   ??relationshipNameNode rdf:type archimate:Name;
           archimate:name ?archimateName;
           xml:lang "en".

} where {

  # Get the ontoarchi metadata of the ontoarchi-relationship
  $this ?relationship ?relatedElement.
  ?relationship rdfs:subPropertyOf ontoarchi:relationship;
    skos:prefLabel ?archimateName;
    xsi:type ?xsiType.
  

''';
    rdfs:isDefinedBy ontoarchi:.

shp:Document
    a sh:NodeShape;
    sh:rule rule:Document;
    sh:targetClass ontoarchi:Model;
    skos:prefLabel 'document shape'@en;
    rdfs:isDefinedBy ontoarchi:.

rule:Document
    a sh:SPARQLRule;
    rdfs:comment 'A rule that creates an archimate document based on an ontoarchi model.'@en;
    skos:prefLabel 'ontoarchi document rule'@en;
    sh:construct '''

prefix archimate: <https://data.rijksfinancien.nl/archimate/model/def/>
prefix function: <https://data.rijksfinancien.nl/ontoarchi/model/function/>
prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>
prefix ontoarchi: <https://data.rijksfinancien.nl/ontoarchi/model/def/>
prefix xml: <http://www.w3.org/XML/1998/namespace#>
prefix xsd: <http://www.w3.org/2001/XMLSchema#>
prefix xsi: <http://www.w3.org/2001/XMLSchema-instance#>

construct {

  # create the ontoarchi-fragment for the ontoarchi-relationship
  ?document rdf:type archimate:Document;
         rdf:_1 ?model.
  ?model rdf:type archimate:Model;
         xmlns:xsi "http://www.w3.org/2001/XMLSchema-instance" ;
         xsi:schemaLocation "http://www.opengroup.org/xsd/archimate/3.0/ http://www.opengroup.org/xsd/archimate/3.1/archimate3_Model.xsd" ;
         xml:xmlns "http://www.opengroup.org/xsd/archimate/3.0/" ;
         archimate:identifier ?modelIdentifier ;
         archimate:version ?modelVersion ;
         rdf:_1 ?modelNameNode;
         rdf:_2 ?modelDocumentation;
         rdf:_3 ?elementsNode;
         rdf:_4 ?relationshipsNode;
         rdf:_5 ?organizationsNode;
         rdf:_6 ?propertiesNode;
         rdf:_7 ?propertyDefinitionsNode;
         rdf:_8 ?viewsNode.
  ?modelNameNode rdf:type archimate:Name;
         archimate:name ?archimateNameModel;
         xml:lang "en".
  ?modelDocumentation rdf:type archimate:Documentation;
         rdf:_1 ?documentationNameNode.
  ?documentationNameNode rdf:type archimate:Name;
         archimate:name ?archimateNameDocumentation;
         xml:lang "en".
  ?elementsNode rdf:type archimate:Elements.
  ?relationshipsNode rdf:type archimate:Relationships.
  ?organizationsNode rdf:type archimate:Organizations.
  ?propertiesNode rdf:type archimate:Properties.
  ?propertyDefinitionsNode rdf:type archimate:PropertyDefinitions.
  ?viewsNode rdf:type archimate:Views.

} where {

  # Get the ontoarchi metadata of the ontoarchi-relationship
  $this ?relationship ?relatedElement.
  ?relationship rdfs:subPropertyOf ontoarchi:relationship;
    skos:prefLabel ?archimateName;
    xsi:type ?xsiType.
  

''';
    rdfs:isDefinedBy ontoarchi:.
