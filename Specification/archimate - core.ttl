    skos:prefLabel 'view node'@en;
    rdfs:isDefinedBy archimate:.
    
archimate:ViewReference a owl:Class;
    dct:identifier "Viewreference";
    skos:definition 'A representation of a system from the perspective of a related set of concerns.'@en;
    skos:prefLabel 'architecture view reference'@en;
    rdfs:isDefinedBy archimate:.
    
archimate:Note a owl:Class;
    dct:identifier "Note";
    skos:definition 'A representation of text in an architecture view.'@en;
    skos:prefLabel 'note'@en;
