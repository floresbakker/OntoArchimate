### Relationship

# Aggregate
archiSVG:Aggegrate a svg:G, archiSVG:Template;
    skos:prefLabel "SVG-based icon for the relationship Aggregate";
    foaf:depicts archimate:aggregation;
    rdf:_1 archiSVG:AggregateTail;
    rdf:_2 archiSVG:AggregateLine;
    rdf:_3 archiSVG:AggregateHead.

archiSVG:AggregateTail a svg:G, archiSVG:Template;
    skos:prefLabel 'dummy SVG element'.

archiSVG:AggregateLine a svg:Polyline, archiSVG:Template;
    svg:fill "none" ;
    svg:points "0,0" ;
    svg:stroke "black" ;
    svg:stroke-width "1" .

archiSVG:AggregateHead a svg:Polyline, archiSVG:Template;
    svg:fill "none" ;
    # 0,0 10,-5 20,0 10,5 0,0 (rotate)
    svg:points "0,0 -5,10 0,20 5,10, 0,0" ;
    svg:stroke "black" ;
    svg:stroke-width "1" .
    
# Access: Read/Write
archiSVG:ReadWrite a svg:G, archiSVG:Template;
    skos:prefLabel "SVG-based icon for the relationship Access: Read/Write";
    foaf:depicts archimate:readWriteAccess;
    rdf:_1 archiSVG:ReadWriteTail;
    rdf:_2 archiSVG:ReadWriteLine;
    rdf:_3 archiSVG:ReadWriteHead.
    
archiSVG:ReadWriteTail a svg:Polyline, archiSVG:Template;
    svg:fill "none" ;
    svg:points "0,0 4,8 0,0 -4,8"; 
    svg:stroke "black" ;
    svg:stroke-width "1" .

archiSVG:ReadWriteLine a svg:Polyline, archiSVG:Template;
    svg:fill "none" ;
    svg:points "0,0" ;
    svg:stroke "black" ;
    svg:stroke-width "1" ;
    svg:stroke-dasharray "4".

archiSVG:ReadWriteHead a svg:Polyline, archiSVG:Template;
    svg:fill "none" ;
    svg:points "0,0 4,8 0,0 -4,8" ;
    svg:stroke "black" ;
    svg:stroke-width "1" .

