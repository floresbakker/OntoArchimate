archiSVG:document a svg:Document  , archiSVG:Template ;
    rdf:_1 archiSVG:svg.

archiSVG:svg a svg:Svg  , archiSVG:Template ;
    xml:xmlns "http://www.w3.org/2000/svg";
    svg:height  "600";
    svg:width  "1200";
    rdf:_1 archiSVG:definitions;
    rdf:_2 archiSVG:genericElements;
    rdf:_3 archiSVG:applicationLayer; 
    rdf:_4 archiSVG:businessLayer;
    rdf:_5 archiSVG:motivationElements;
    rdf:_6 archiSVG:relationships.

archiSVG:definitions rdf:type svg:G, archiSVG:Definitions, archiSVG:Template ;
    skos:prefLabel 'Template for SVG definitions';
    rdf:_1 archiSVG:relationDefinitions;
    rdf:_2 archiSVG:iconDefinitions.
    
archiSVG:relationDefinitions rdf:type svg:G, archiSVG:Template ;
    skos:prefLabel 'Template for SVG definitions of relations';
    rdf:_1 archiSVG:association;
    rdf:_2 archiSVG:aggregation;
    rdf:_3 archiSVG:composition;
    rdf:_4 archiSVG:read;
    rdf:_5 archiSVG:write;
    rdf:_6 archiSVG:serving;
    rdf:_7 archiSVG:realization;
    rdf:_8 archiSVG:assignmentStart;
    rdf:_9 archiSVG:assignmentEnd;
    rdf:_10 archiSVG:specialization;
    rdf:_11 archiSVG:influence.    

# Definition of relations 
    
archiSVG:aggregation rdf:type svg:Marker, archiSVG:Definition ;
    svg:id "aggregation";
    svg:markerWidth "20";
    svg:markerHeight "20";
    svg:refX "0";
    svg:refY "5";
    svg:orient "auto";
    svg:markerUnits "strokeWidth";
    rdf:_1 archiSVG:aggregationStart.

archiSVG:aggregationStart rdf:type svg:Polyline, archiSVG:Definition ;
    svg:fill "white" ;
    svg:points "10,0 0,5 10,10 20,5 10,0" ;
    svg:stroke "black" ;
    svg:stroke-width "1" .
    
archiSVG:association rdf:type svg:Marker, archiSVG:Definition ;
    svg:id "association";
    svg:markerWidth "20";
    svg:markerHeight "20";
    svg:refX "0";
    svg:refY "5";
    svg:orient "auto";
    svg:markerUnits "strokeWidth";
    rdf:_1 archiSVG:associationStart.

archiSVG:associationStart rdf:type svg:G, archiSVG:Definition ;
    rdfs:comment 'The association relationship is represented as a visual line in the Archimate language without a special visual characteristic. Hence the use of a empty SVG:G element, which through the use of the archiSVG serialisation vocabulary will result in the desired bare line.';
    svg:fill "none" ;
    svg:stroke "black" ;
    svg:stroke-width "1" .    
 
archiSVG:composition rdf:type svg:Marker, archiSVG:Definition ;
    svg:id "composition";
