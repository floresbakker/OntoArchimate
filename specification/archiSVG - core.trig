# Product

archiSVG:Product a svg:G , archiSVG:Template ;
    foaf:depicts archimate:Product;
    skos:prefLabel "SVG depiction of a product in Archimate";
    rdf:_1 archiSVG:ProductBox;
    rdf:_2 archiSVG:ProductIconUse;
    rdf:_3 archiSVG:ProductText.

archiSVG:ProductBox a svg:Path  , archiSVG:Template ;
    svg:fill "rgb(255,255,181)" ;
    svg:d "M 0,0 H 144 V 55 H 0 Z";
    svg:stroke "black" .

archiSVG:ProductIconUse rdf:type svg:Use, archiSVG:Template ;
    xlink:href "#product".
    
archiSVG:ProductText a svg:TextElement , archiSVG:Template ;
    rdf:_1 archiSVG:ProductTextFragment ;
    svg:font-family "Segoe UI" ;
    svg:font-size "9" ;
    svg:x "50" ;
    svg:y "15" .

archiSVG:ProductTextFragment a svg:Text  , archiSVG:Template ;
    xml:fragment "Product" .
    
